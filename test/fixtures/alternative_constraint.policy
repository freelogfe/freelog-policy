for ALL:

  in state_1:
    proceed to state_2 on event_1
  in state_2:
    proceed to state_3 on event_2
  in state_3:
    proceed to state_2 on event_3
    terminate on event_4

  define state_2:
    recontractable with conditions:
      target SELF
      exclude event type payment, settlement
      enforce agreement <resource_id>
  define state_3:
    recontractable 
